
public class User {
	   private String LName;
	   private String Fname;
	   private String Type;
	   
	   public String getLName()
	   {
		   return LName;
	   }
	   public String getFname()
	   {
		   return Fname;
	   }
	   public  String getPostalCode()
	   {
		   return PostalCode;
	   }
	   public void setLName(String LName)
	   {
		   this.LName = LName;
