public class Property {
   private int Area;
   private int Cost;
   private String PostalCode;
   
   public int getArea()
   {
	   return Area;
   }
   public int getCost()
   {
	   return Cost;
   }
   public  String getPostalCode()
   {
	   return PostalCode;
   }
   public void setArea(int Area)
   {
	   this.Area = Area;
   }
   public void setCost(int Cost)
   {
	   this.Cost = Cost;
   }
   public  void setPostalCode()
   {
	   return PostalCode;
   }
   
}
