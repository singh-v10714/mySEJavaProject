public class Property {
   private int Area;
   private int Cost;
   private String PostalCode;
   
   public int getArea()
   {
	   return Area;
   }
   public int getCost()
   {
	   return Cost;
   }
   public  String getPostalCode()
   {
	   return Area;
   }
   public void setArea()
   {
	   return Area;
   }
   public void setCost()
   {
	   return Cost;
   }
   public  vpoid setPostalCode()
   {
	   return PostalCode;
   }
   
}
